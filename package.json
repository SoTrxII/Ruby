{
  "name": "ruby",
  "version": "1.2.0",
  "scripts": {
    "packRelease": "tar -zcvf Ruby.tgz dist",
    "build": "cp -n config_template.json config.json && tsc --sourceMap false && cp package.json dist/package.json",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "test": "node ./node_modules/mocha/bin/_mocha --require ts-node/register test/**/*.ts",
    "dev": "mv config.json config_prod.json && mv config_dev.json config.json",
    "prod": "mv config.json config_dev.json && mv config_prod.json config.json"
  },
  "description": "Ruby",
  "main": "src/rin.ts",
  "repository": "https://github.com/Soulcramer/Rin.git",
  "author": "Soulcramer, SoTrx",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.0.1",
    "discord.js": "^11.4.2",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.2",
    "googleapis": "^43.0.0",
    "node-opus": "^0.3.0",
    "pcm-volume": "^1.0.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0",
    "sodium": "^3.0.2",
    "speaker": "^0.4.2",
    "uws": "^100.0.1",
    "ytdl-core": "^0.29.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/debug": "^4.1.5",
    "@types/fluent-ffmpeg": "^2.1.10",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/ramda": "^0.26.21",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "inversify": "^5.0.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "sinon": "^7.4.2",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ]
  }
}
