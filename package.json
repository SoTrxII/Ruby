{
  "name": "ruby",
  "version": "2.1.0",
  "scripts": {
    "packRelease": "tar -zcvf Ruby.tgz dist",
    "build": "cp speakertypes.bak node_modules/speaker/index.d.ts &&  cp -n config_template.json config.json && node_modules/.bin/tsc --sourceMap false && cp package.json dist/package.json",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "test": "node ./node_modules/mocha/bin/_mocha --require ts-node/register --require reflect-metadata/Reflect \"src/**/*.spec.ts\"",
    "dev": "mv config.json config_prod.json && mv config_dev.json config.json",
    "prod": "mv config.json config_dev.json && mv config_prod.json config.json"
  },
  "description": "Ruby",
  "main": "src/rin.ts",
  "repository": "https://github.com/Soulcramer/Rin.git",
  "author": "Soulcramer, SoTrx",
  "license": "MIT",
  "dependencies": {
    "@discordjs/uws": "^10.149.0",
    "debug": "^4.0.1",
    "discord.js": "^11.6.1",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.6",
    "googleapis": "^47.0.0",
    "inversify": "^5.0.1",
    "node-opus": "^0.3.3",
    "pcm-volume": "^1.0.0",
    "ramda": "^0.27.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "sodium": "^3.0.2",
    "speaker": "^0.5.1",
    "tslib": "^1.11.1",
    "ytdl-core": "^2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/debug": "^4.1.5",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.7",
    "@types/ramda": "^0.26.43",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.0",
    "prettier": "^1.19.1",
    "sinon": "^9.0.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ]
  }
}
